  display: block;
  content: "...\A..\A.";
  white-space: pre-wrap;
  animation: _dot 3s infinite step-start both;
}
@keyframes _dot {
  33% {
    transform: translateY(-2em);
  }
  66% {
    transform: translateY(-1em);
  }
}
